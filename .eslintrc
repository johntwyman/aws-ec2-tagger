{
  "parser": "@babel/eslint-parser",
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-restricted-syntax": "off",
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-use-before-define": [
      "error",
      {
        "variables": true,
        "functions": false,
        "classes": false
      }
    ]
  }
}